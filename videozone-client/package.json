      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "autoprefixer": "^9.7.4",
    "flow-bin": "^0.121.0",
    "postcss-cli": "^7.1.0",
